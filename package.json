{
  "name": "tampermonkey-comic",
  "version": "1.0.4",
  "description": "油猴漫画插件",
  "author": "journey3510",
  "private": true,
  "main": "/src/main.js",
  "keywords": [
    "tampermonkey",
    "webpack",
    "vue",
    "vant"
  ],
  "scripts": {
    "dev": "dotenv -e .env.dev -- webpack-dev-server --open --config webpack.dev.js",
    "build": "dotenv -e .env -- webpack --config webpack.prod.js && dotenv -e .env -- node build.js",
    "build:dev": "dotenv -e .env.dev -- webpack --config webpack.prod.js && dotenv -e .env.dev -- node build.js",
    "test": "dotenv -e .env.test -- webpack --config webpack.test.js --watch"
  },
  "license": "MIT",
  "dependencies": {
    "core-js": "^3.23.3",
    "jszip": "^3.10.0",
    "vant": "^2.12.48",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "dotenv-cli": "^4.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "node-sass": "^6.0.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "vue-loader": "^15.9.5",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.4.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  }
}
